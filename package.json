{
  "name": "React-redux-socketio-chat",
  "version": "0.3.0",
  "description": "react frontend, socketio powered chat. redux as the flux implementation",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "node server",
    "dev": "set NODE_ENV=development node devServer",
    "test": "mocha --compilers js:babel/register --recursive",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src",
    "build:webpack": "set NODE_ENV=production && webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raineroviir/react-redux-socketio-chat"
  },
  "keywords": [
    "react",
    "reactjs",
    "webpack",
    "babel",
    "redux",
    "socket.io",
    "redux-devtools",
    "redux-promise",
    "react-transform"
  ],
  "author": "roviir@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raineroviir/react-redux-socketio-chat/issues"
  },
  "homepage": "https://github.com/raineroviir/react-redux-socketio-chat",
